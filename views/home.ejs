<%- include('partials/header') -%>

<h1>Home</h1>
<p> <%= startingContent %></p>

<div class="New Post">
    <% posts.forEach(function(post) { %>
        <h1 class="title"> <%= post.title %> </h1>
        <p class="content"> <%= post.content.substring(0,100)+ " ..." %>
        <a href="posts/<%=post.title %>">Read More</a></p>
    <% }) %>
</div>


 

<%- include('partials/footer') -%>